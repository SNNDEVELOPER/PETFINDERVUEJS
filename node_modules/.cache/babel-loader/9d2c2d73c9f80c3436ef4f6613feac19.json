use Trained: {{ petData.attributes.house_trained }}\nSpecial Needs: {{ petData.attributes.special_needs }}\nHEALTH {{ petData.attributes.shots_current }}\nEnvironment {{ petData.environment }}\n                    <p v-html=\"petData.description\"></p>\n                    \n                    <div class=\"petButtons\">\n                        <p><a href=\"${pet[i].url}\">Adopt {{ petData.name }}</a></p>\n                        <p><router-link :to=\"{ name: 'pets' }\">See more pets</router-link></p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"petLeft\">\n                <div class=\"petImageGallery\" v-if=\"petData.photos\">\n                     <div class=\"petMainImage\" :style=\"{ backgroundImage: 'url(' + petImage + ')' }\">\n                    </div>\n                   \n                    <div class=\"petThumbs\">\n                     <div\n                        v-for=\"photos in petData.photos\" \n                        :key=\"photos.id\" \n                        class=\"thumbs\"\n                    >\n                       <img :src=\"photos.small\" class=\"petThumbnail\" @click=\"showLargeImage(photos.medium)\"> \n                    </div>\n                    </div>\n\n                </div>\n            </div>\n        </div>\n    </div>\n\n  </div>\n\n</template>\n\n\n<script>\nimport axios from 'axios';\nimport petChars from './petChars';\n\nexport default {\n  name: 'petfinderDetails',\n  components: { \n    'appPetChars': petChars\n  },\n  props: {\n    \n  },\n  data() {\n      return {\n          petData: {},\n          loading: true,\n          errors: false,\n          petImage: '',\n      }\n  },\n  mounted() {\n    axios\n      .get('http://www.shaunnelson.com/skunkworks/petfindertest/getpets.php')\n      .then(response => (this.petData = this.getPet(response.data.animals)))\n      .catch(error =>  {\n          console.log(error)\n          this.errors = true\n      })\n      .finally( () => this.loading = false)\n  },\n  methods: {\n    getPet: function(petData) {\n      petData = [petData.find(el => el.id === parseInt(this.$route.query.petID))];\n      this.petImage = petData[0].photos[0].medium;\n      return petData[0];\n    },\n    buildThumbs: function(photos) {\n        let thumbnails = ``;\n        for (let i = 0; i < photos.length; i++) {\n            thumbnails += `<div class=\"thumbs\">\n                            <img src=\"${photos[i].small}\" class=\"petThumbnail\" data-fullsize=\"${photos[i].medium}\">\n                           </div>`\n        }\n        return thumbnails;\n    },\n    showLargeImage: function(img) {\n      this.petImage = img;\n    },\n    removeNull: function(obj) {\n            obj.forEach(function (ob) {\n                Object.keys(ob).forEach(function (k) {\n                    if (ob[k] === null) {\n                        ob[k] = '';\n                    }\n                });\n            });\n            \n            return obj;\n        }\n  }\n}\n</script>\n\n\n<style scoped>\n\n</style>\n\n"],"sourceRoot":"src/components"}]}